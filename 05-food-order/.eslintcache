/Cart/CartIcon\";\r\nimport CartContext from \"../../store/cart-context\";\r\n\r\nconst HeaderCartButton = (props) => {\r\n  const [btnHighlighted, setBtnHiglighted] = useState(false);\r\n  const cartCtx = useContext(CartContext);\r\n  const { items } = cartCtx;\r\n\r\n  const cartItemsCount = items.reduce((curNumber, item) => {\r\n    return curNumber + item.amount;\r\n  }, 0);\r\n\r\n  const btnClasses = \"hcb__button\" + `${btnHighlighted ? \" hcb__bump\": \"\"}`\r\n\r\n  useEffect(() => {\r\n    if (items.length === 0) {\r\n      return\r\n    }\r\n    setBtnHiglighted(true);\r\n\r\n    const timer = setTimeout(() => {\r\n      setBtnHiglighted(false);\r\n    }, 300)\r\n\r\n    return () => {\r\n      clearTimeout(timer);\r\n    }\r\n  }, [items])\r\n\r\n  return (\r\n    <button onClick={props.onClick} className={btnClasses}>\r\n      <span className=\"hcb__icon\">\r\n        <CartIcon />\r\n      </span>\r\n      <span>Cart</span>\r\n      <span className=\"hcb__badge\">{cartItemsCount}</span>\r\n    </button>\r\n  );\r\n};\r\n\r\nexport default HeaderCartButton;\r\n","C:\\Users\\eomer\\Desktop\\Future\\react\\05-food-order\\src\\components\\Cart\\CartIcon.jsx",[],"C:\\Users\\eomer\\Desktop\\Future\\react\\05-food-order\\src\\constants\\dummy_meals.js",[],"C:\\Users\\eomer\\Desktop\\Future\\react\\05-food-order\\src\\components\\Meals\\Meals.jsx",[],"C:\\Users\\eomer\\Desktop\\Future\\react\\05-food-order\\src\\components\\Meals\\AvailableMeals.jsx",["123","124"],"import React, { useEffect, useState } from \"react\";\r\nimport useHttp from \"../../hooks/use-http\";\r\nimport Card from \"../UI/Card\";\r\n\r\nimport \"./AvailableMeals.css\";\r\nimport MealItem from \"./MealItem/MealItem\";\r\n\r\nconst AvailableMeals = (props) => {\r\n  const { loading, error, sendRequest: fetchMeals } = useHttp();\r\n  const dbUrl = \"https://learn-react-7c3cf-default-rtdb.europe-west1.firebasedatabase.app/meals.json\";\r\n\r\n  useEffect(() => {\r\n    const processMeals = (data) => {\r\n      let updatedMeals = [];\r\n\r\n      for (const key in data) {\r\n        updatedMeals.push({\r\n          key: data[key].id,\r\n          id: data[key].id,\r\n          name: data[key].name,\r\n          description: data[key].description,\r\n          price: data[key].price,\r\n          tags: data[key].tags,\r\n        });\r\n      }\r\n\r\n      props.onListMeals(updatedMeals);\r\n    };\r\n\r\n    fetchMeals({ url: dbUrl }, processMeals);\r\n  }, [fetchMeals]);\r\n\r\n  const mealsList = props.meals.map((meal) => (\r\n    <MealItem\r\n      key={meal.key}\r\n      id={meal.id}\r\n      name={meal.name}\r\n      description={meal.description}\r\n      price={meal.price}\r\n      tags={meal.tags}\r\n    />\r\n  ));\r\n\r\n  return (\r\n    <section className=\"available_meals\">\r\n      <Card>\r\n        {loading && !error ? <p>Loading...</p> : <ul>{mealsList}</ul>}\r\n        {error && <p className=\"available_meals-error_text\">{error}</p>}\r\n      </Card>\r\n    </section>\r\n  );\r\n};\r\n\r\nexport default AvailableMeals;\r\n","C:\\Users\\eomer\\Desktop\\Future\\react\\05-food-order\\src\\components\\Meals\\MealsSummary.jsx",[],"C:\\Users\\eomer\\Desktop\\Future\\react\\05-food-order\\src\\components\\UI\\Card.jsx",[],"C:\\Users\\eomer\\Desktop\\Future\\react\\05-food-order\\src\\components\\Meals\\MealItem\\MealItem.jsx",[],"C:\\Users\\eomer\\Desktop\\Future\\react\\05-food-order\\src\\components\\Meals\\MealItem\\MealItemForm.jsx",[],"C:\\Users\\eomer\\Desktop\\Future\\react\\05-food-order\\src\\components\\UI\\Input.jsx",[],"C:\\Users\\eomer\\Desktop\\Future\\react\\05-food-order\\src\\components\\Cart\\Cart.jsx",[],"C:\\Users\\eomer\\Desktop\\Future\\react\\05-food-order\\src\\components\\UI\\Modal.jsx",[],"C:\\Users\\eomer\\Desktop\\Future\\react\\05-food-order\\src\\components\\UI\\Buttons\\Button.jsx",[],"C:\\Users\\eomer\\Desktop\\Future\\react\\05-food-order\\src\\components\\UI\\Buttons\\ButtonBInv.jsx",[],"C:\\Users\\eomer\\Desk